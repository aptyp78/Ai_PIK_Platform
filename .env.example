# Copy this file to .env and fill real values. Do NOT commit .env

# API keys
OPENAI_API_KEY=

# Core paths (adjust to your machine)
MODEL_DIR=/root/models
DATA_DIR=/root/data

# Playbook inputs
PDF_PATH=/root/data/playbook.pdf
JSON_PATH=/root/data/playbook.json

# Output artifacts
OUT_PAGES=out/page_images/PIK - Expert Guide - Platform IT Architecture - Playbook - v11
OUT_DET=out/visual/grounded_regions
INDEX_PATH=out/openai_embeddings.ndjson

# Models and options
CHAT_MODEL=gpt-4o
EMB_MODEL=text-embedding-3-large
# Disable legacy CV segmentation in favor of GroundedDINO+SAM2
USE_CV=false
